#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macbookpro/blog/node_modules/.pnpm/@sanity+cli@3.12.2/node_modules/@sanity/cli/bin/node_modules:/Users/macbookpro/blog/node_modules/.pnpm/@sanity+cli@3.12.2/node_modules/@sanity/cli/node_modules:/Users/macbookpro/blog/node_modules/.pnpm/@sanity+cli@3.12.2/node_modules/@sanity/node_modules:/Users/macbookpro/blog/node_modules/.pnpm/@sanity+cli@3.12.2/node_modules:/Users/macbookpro/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macbookpro/blog/node_modules/.pnpm/@sanity+cli@3.12.2/node_modules/@sanity/cli/bin/node_modules:/Users/macbookpro/blog/node_modules/.pnpm/@sanity+cli@3.12.2/node_modules/@sanity/cli/node_modules:/Users/macbookpro/blog/node_modules/.pnpm/@sanity+cli@3.12.2/node_modules/@sanity/node_modules:/Users/macbookpro/blog/node_modules/.pnpm/@sanity+cli@3.12.2/node_modules:/Users/macbookpro/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sanity/cli/bin/sanity" "$@"
else
  exec node  "$basedir/../@sanity/cli/bin/sanity" "$@"
fi
